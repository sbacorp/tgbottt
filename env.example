# Основные настройки бота
BOT_TOKEN=your_telegram_bot_token_here
DATABASE_URL=postgresql://username:password@localhost:5432/database_name
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
CLAUDE_API_KEY=your_anthropic_claude_api_key_here

# Настройки мониторинга
MONITORING_INTERVAL=2700000
LOG_LEVEL=info

# Администраторы (через запятую)
ADMIN_USER_IDS=123456789,987654321

# Настройки прокси для Playwright
# Включить/выключить прокси
PROXY_ENABLED=false

# Адрес прокси-сервера (например: http://proxy.example.com:8080 или socks5://proxy.example.com:1080)
PROXY_SERVER=http://proxy.example.com:8080

# Имя пользователя для аутентификации прокси (если требуется)
PROXY_USERNAME=proxy_username

# Пароль для аутентификации прокси (если требуется)
PROXY_PASSWORD=proxy_password

# Исключения для прокси (через запятую, например: localhost,127.0.0.1,*.example.com)
PROXY_BYPASS=localhost,127.0.0.1
